{
  "extends": ["prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": [2, "always"],
    "@typescript-eslint/ban-ts-comment": "off"
  }
}
